<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dbService.entity.SuperheroesEntitySQLite" table="SUPERHEROES">
        <id name="heroName">
            <column name="HERO_NAME" sql-type="text" not-null="true"/>
        </id>
        <property name="universe">
            <column name="UNIVERSE" sql-type="text" not-null="true"/>
        </property>
        <property name="power">
            <column name="POWER" sql-type="integer" not-null="true"/>
        </property>
        <property name="description">
            <column name="DESCRIPTION" sql-type="text" />
        </property>
        <property name="isAlive">
            <column name="IS_ALIVE" sql-type="text" />
        </property>
        <property name="imagePath">
            <column name="IMAGE_PATH" sql-type="text" />
        </property>
        <property name="phone">
            <column name="PHONE" sql-type="text"  />
        </property>
    </class>
</hibernate-mapping>